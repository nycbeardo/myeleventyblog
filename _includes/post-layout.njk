---
layout: base-layout.njk
---
<article>
  <h1>{{ title }}</h1> 
  <time datetime="{{ date | dateIso }}">{{ date | dateReadable }}</time> 
  <h3 class="tagtitle">Tags:</h3>
  <ul>
  {% for tag in tags %}
  <li>
  <a class="tags" href="">{{ tag }}</a>
  </li>
  {% endfor %}
  </ul>
  {{ content | safe }}
</article>

